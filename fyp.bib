@INPROCEEDINGS{5918041, 
    author={Y. l. Zhu and R. Li and X. b. Liu and Xu Jian}, 
    booktitle={Business Management and Electronic Information (BMEI), 2011 International Conference on}, 
    title={Wireless communication technology in family health monitoring system}, 
    year={2011}, 
    volume={3}, 
    pages={64-67}, 
    abstract={Base on the analysis of traditional long-range medical healthcare system, the human body healthcare system
        used in the family is introduced in this paper. According to the requirements of family health monitor system,
        the paper compares several kinds of wireless communication technology which uses in the family. The system is
            presented to combine the short-range wireless communication technology Zigbee and the long-range wireless
            communication technology GPRS to monitor human health. This system can not only meet the main requirement of
            real-time monitoring, but also avoid the costly nursing expenses.}, 
    keywords={Zigbee;biomedical communication;health care;packet radio networks;patient monitoring;ubiquitous
        computing;GPRS;Zigbee;family health monitoring system;human body healthcare system;long range medical healthcare
            system;wireless communication technology;Biomedical monitoring;Ground penetrating radar;Medical
            services;Monitoring;Wireless communication;Wireless sensor networks;Zigbee;GPRS;health monitor;wireless
            communication;zigbee}, 
    doi={10.1109/ICBMEI.2011.5918041}, 
