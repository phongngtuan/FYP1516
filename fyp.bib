@INPROCEEDINGS{7059154, 
    author={I. Shabani and A. Kova√ßi and A. Dika}, 
    booktitle={Computational Intelligence, Communication Systems and Networks (CICSyN), 2014 Sixth International
        Conference on}, 
    title={Possibilities Offered by Google App Engine for Developing Distributed Applications Using Datastore}, 
    year={2014}, 
    pages={113-118}, 
    abstract={Efficient and scalable simultaneous interaction between many web servers requires special handling of such
        interaction. Google Datastore provides highly available and reliable distributed storage and access on different
            types of data. With the rapid growth of distributed systems, efficient data provide is of critical
            importance. In this paper we present the possibilities which Google Application Engine and cloud computing
            provides to build distributed applications by specifically discussing the options to store the data on the
            cloud in a reliable way. Important is identification of the programming languages and tools which are
            suitable for designing of distributed systems using the Google possibilities as a framework for developing
            such systems. The design of distributed systems by using Google features together with the libraries in Java
            and Python programming language are examined in this paper.}, 
    keywords={Java;cloud computing;distributed processing;relational databases;storage management;Google Application
        Engine;Google Datastore;Google possibilities;Java;Python programming language;Web servers;cloud
            computing;distributed applications;distributed data access;distributed data storage;programming
            languages;programming tools;Cloud computing;Computers;Engines;Google;Hardware;Java;Servers;BigTable;Cloud
            Computing;Datastore;Destributed application}, 
    doi={10.1109/CICSyN.2014.35}, 
    month={May},
}

